_shader
	vs	shaders/geometry.vs
	fs	shaders/pbr.fs
_end_shader

_state
	depth_write on
	depth_test on less_or_equal
	cull_face on back ccw
	blending off
_end_state

_params
	_p_ub_holder ub_Scene
	_p_ub_holder ub_Lighting
	
	_p_tex u_AlbedoTex
		dim				tex2d
		min_filter		linear_mipmap_linear
		mag_filter		linear
		wrap_s			repeat
		wrap_t			repeat
		path			textures/17876391417123941155.cbtex
	_end_p_tex
	
	_p_tex u_NormalTex
		dim				tex2d
		min_filter		linear_mipmap_linear
		mag_filter		linear
		wrap_s			repeat
		wrap_t			repeat
		path			textures/6593109234861095314.cbtex
	_end_p_tex

	_p_tex u_AttribTex
		dim				tex2d
		min_filter		linear_mipmap_linear
		mag_filter		linear
		wrap_s			repeat
		wrap_t			repeat
		path			textures/466164707995436622.cbtex
	_end_p_tex
	
	_p_tex u_PMREMTex
		dim				texcube
		min_filter		linear_mipmap_linear
		mag_filter		linear
		wrap_s			clamp_to_edge
		wrap_t			clamp_to_edge
		wrap_r			clamp_to_edge
		path			env/venice_sunset.cbtex
	_end_p_tex
	
	_p_tex_holder tex2d u_SplitSumTex
	_p_tex_holder tex2d u_ShadowMap
_end_params